
## defines qwthome
QWTHOME = "../../3rdParty/qwt/build"



QMAKE_LFLAGS += -enable-auto-import
QMAKE_CXXFLAGS_WARN_OFF += -Wunused-parameter

DEFINES += __x86__ \
    __NT__ \
    __OSVERSION__=4 \
    __WIN32__ \
    WIN32 \
    _WINDOWS \
    _USRDLL \
    _CRT_SECURE_NO_DEPRECATE \
    _WIN32_WINNT=0x0400 \
    _MBCS \
    QWT_DLL



INCLUDEPATH += $$QWTHOME/include
